#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b,c,d,t;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    (a>b)?(t=a,a=b,b=t):0;
    (a>c)?(t=a,a=c,c=t):0;
    (a>d)?(t=b,b=c,c=t):0;
    (b>c)?(t=b,b=c,c=t):0;
    (b>d)?(t=b,b=d,d=t):0;
    (c>d)?(t=c,c=d,d=t):0;
    printf("%d %d %d %d",a,b,c,d);
    
    
    return 0;
}
